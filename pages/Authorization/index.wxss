page {
  width: 100%;
  height: 100%;
}
.box {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
image {
  display: inline-block;
  margin-top: 10rpx;
  width: 36rpx;
  height: 36rpx;
}
.regbutton {
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  padding: 20rpx;
  border-radius: 40rpx;
  background-color: orange;
}
text {
  display: inline-block;
  margin-left: 10rpx;
}